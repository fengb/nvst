%div#growth-wrapper

- content_for(:tail) do
  :javascript
    d3.json('/portfolio.json', function(error, data) {
      if (error) return console.warn(error);

      InvestmentGraph('#growth-wrapper', 'value')
        .add('Portfolio', data.portfolio)
        .add('S&P 500', data.benchmark)
        .render()
    })
